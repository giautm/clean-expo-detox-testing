    "simulator": {
      "type": "ios.simulator",
      "device": {
        "type": "iPhone 11"
      }
    }
  },
  "configurations": {
    "android": {
      "app": "android",
      "device": "emulator"
    },
    "ios": {
      "app": "ios",
      "device": "simulator"
    }
  }
