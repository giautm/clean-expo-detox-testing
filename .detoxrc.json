{
  "testRunner": "jest",
  "runnerConfig": "e2e/config.json",
  "skipLegacyWorkersInjection": true,
  "apps": {
    "android": {
      "type": "android.apk",
      "binaryPath": "e2e/bin/Exponent.apk",
      "testBinaryPath": "e2e/bin/Exponent.apk"
    },
    "ios": {
      "type": "ios.app",
      "binaryPath": "e2e/bin/Exponent.app"
    }
  },
  "devices": {
    "emulator": {
      "type": "android.emulator",
